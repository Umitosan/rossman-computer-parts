<%= render('layouts/errors', :object => @product ) %>

<%= form_for [@product] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :cost %>
  <%= f.text_field :cost %>
  <br><br>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <br><br>
  <p><%= f.submit %></p>
<% end %>
